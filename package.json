{
  "name": "openfn-vscode",
  "displayName": "OpenFn VSCode",
  "description": "openfn language support for vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "homepage": "https://github.com/OpenFn/vscode",
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenFn/vscode"
  },
  "bugs": {
    "url": "https://github.com/OpenFn/vscode/issues"
  },
  "activationEvents": [
    "workspaceContains:**/.openfnrc"
  ],
  "main": "./out/extension.js",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9"
  }
}